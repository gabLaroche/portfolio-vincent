    "classnames": "^2.2.6",
    "next": "^11.1.0",
    "next-fonts": "^1.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-lazy-load-image-component": "^1.4.3",
    "react-masonry-layout": "^0.4.0",
    "react-progressive-graceful-image": "^0.6.11",
    "sass": "^1.27.0"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
